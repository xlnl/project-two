<% if(province && province.tips) { %>
    <% province.tips.forEach((tip) => { %>
      <div>
        <h3>
            Tip Id: <%= tip.id %> . 
            <br>
            Tip Description: <%= tip.description %>
            <br>
            <small>Located in: <%= tip.address %></small> 
        </h3>
      </div>
    <% }) %>
<% } else { %>
<h3>No tips from a province yet!</h3>
<% } %>



