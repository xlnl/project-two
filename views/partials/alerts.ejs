<% if (alerts.error) { %>
    <div class="alert alert-warning" role="alert">
    <% alerts.error.forEach(msg => { %>
        <%= msg %>
    <% }) %>
    </div>
<% } %>
<% if (alerts.success) { %>
    <div class="alert alert-success" role="alert">
    <% alerts.success.forEach(msg => { %>
        <%= msg %>
    <% }) %>
    </div>
<% } %>